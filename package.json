{
  "name": "vite-plugin-shared-modules",
  "version": "0.0.1",
  "description": "Alias node_modules to specific one",
  "keywords": [
    "vite-plugin",
    "node_modules",
    "pnpm",
    "monorepo",
    "share",
    "rewrite",
    "alias",
    "resolve"
  ],
  "author": "Zheeeng<hi@zheeeng.me>",
  "license": "MIT",
  "main": "lib/index.js",
  "typing": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zheeeng/vite-plugin-shared-modules"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc -b ./tsconfig.build.json",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix",
    "prepublishOnly": "pnpm build",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "babel-plugin-rewrite-module-path": "^0.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "pnpm": "^6.16.0",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.3"
  }
}
